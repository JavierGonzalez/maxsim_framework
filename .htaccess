DirectorySlash Off

RewriteEngine on
RewriteBase /

RewriteCond %{REQUEST_FILENAME} !-f
RewriteRule .* /$maxsim.php [L]

<FilesMatch "(.php|password)">
 Order Allow,Deny
 Deny from all
</FilesMatch>

<Files $maxsim.php>
 Order Allow,Deny
 Allow from all
</Files>

